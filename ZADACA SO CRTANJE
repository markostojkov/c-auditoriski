#include <stdio.h>
#include <stdlib.h>

int main(int argc, char const *argv[])
{
	int n = 4, m = 6;

	for(int i = 0; i < n; i++){

		for(int j = 0; j < m; j++){
			
			if(i % 2 == 0){
				if( j % 2 == 0)
					printf(".");
				else
					printf("*");
			}
			else{
				if( j % 2 == 0)
					printf("*");
				else
					printf(".");
			}

		}

		printf("\n");
	}

	system("pause");


	return 0;
}
